on boot
	write /proc/sys/net/core/rmem_min 1310720
	write /proc/sys/net/core/wmem_min 1310720
	write /proc/sys/net/core/rmem_default 1310720
	write /proc/sys/net/core/wmem_default 1310720
	write /proc/sys/net/core/rmem_max 2621440
	write /proc/sys/net/core/wmem_max 2621440
	write /sys/power/pm_freeze_timeout 1750

# rmem_default set to 1.25M, rmem_max set to 2.5M
# This was added by Holo
